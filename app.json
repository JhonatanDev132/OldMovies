{
  "expo": {
    "name": "Old Movies",
    "slug": "old-Movies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/scream.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-oldmovies.png",
      "resizeMode": "contain",
      "backgroundColor": "#2b2b2b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-dahora.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
